<?xml version="1.0" encoding="utf-8" ?>
<Configuration status="WARN" xmlns="http://logging.apache.org/log4j/2.0/config">
    <Appenders>
        <Console name="cnsl" target="SYSTEM_OUT">
            <PatternLayout pattern="Log >> %d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="cnsl" />
        </Root>
    </Loggers>
</Configuration>